{
  "name": "jupyterlab_optumi",
  "version": "3.12.5",
  "description": "Offload Jupyter notebooks from your laptop into the cloud",
  "license": "UNLICENSED",
  "private": "true",
  "keywords": [
    "jupyter",
    "jupyterlab",
    "jupyterlab-extension"
  ],
  "homepage": "https://optumi.com",
  "author": {
    "name": "Optumi Inc Authors",
    "email": "cs@optumi.com"
  },
  "files": [
    "lib/**/*.{d.ts,eot,gif,html,jpg,js,js.map,json,png,svg,woff2,ttf}",
    "style/**/*.{css,eot,gif,html,jpg,json,png,svg,woff2,ttf}",
    "style/index.js"
  ],
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "style": "style/index.js",
  "scripts": {
    "build": "jlpm run build:lib && jlpm run build:labextension:dev",
    "build:all": "jlpm run build:lib && jlpm run build:labextension",
    "build:labextension": "jupyter labextension build .",
    "build:labextension:dev": "jupyter labextension build --development True .",
    "build:lib": "tsc",
    "build:prod": "jlpm run clean && jlpm run build:lib && jlpm run build:labextension",
    "clean": "jlpm run clean:lib",
    "clean:all": "jlpm run clean:lib && jlpm run clean:labextension",
    "clean:labextension": "rimraf jupyterlab_optumi/labextension",
    "clean:lib": "rimraf lib tsconfig.tsbuildinfo",
    "eslint": "eslint . --ext .ts,.tsx --fix",
    "eslint:check": "eslint . --ext .ts,.tsx",
    "install:extension": "jlpm run build",
    "postinstall": "echo 'Adding onBlur prop to OverlayTrigger' ; grep -q 'onBlur' node_modules/react-bootstrap/esm/OverlayTrigger.d.ts || sed -i '11 i \\ \\ \\ \\ onBlur?: (...args: any[]) => any;' node_modules/react-bootstrap/esm/OverlayTrigger.d.ts",
    "prepare": "jlpm run clean && jlpm run build:prod",
    "watch": "run-p watch:src watch:labextension",
    "watch:labextension": "jupyter labextension watch .",
    "watch:src": "tsc -w"
  },
  "//": [
    "@babel/core is a peer of @emotion/react",
    "@emotion/react is a peer of @mui/lab and @mui/material",
    "@emotion/styled is a peer of @mui/lab and @mui/material",
    "codemirror is a peer of react-codemirror2",
    "react-dom is a peer of many packages",
    "typanion is a peer of @jupyterlab/builder"
  ],
  "dependencies": {
    "@babel/core": "7.16.12",
    "@emotion/react": "11.9.0",
    "@emotion/styled": "11.8.1",
    "@jupyterlab/application": "3.4.3",
    "@jupyterlab/apputils": "3.4.3",
    "@jupyterlab/coreutils": "5.4.3",
    "@jupyterlab/notebook": "3.4.3",
    "@mui/icons-material": "5.5.1",
    "@mui/lab": "5.0.0-alpha.75",
    "@mui/material": "5.5.3",
    "@mui/styles": "5.5.3",
    "@stripe/stripe-js": "1.29.0",
    "codemirror": "5.65.3",
    "google-libphonenumber": "3.2.27",
    "marked": "3.0.8",
    "moment": "2.29.3",
    "notistack": "2.0.4",
    "plotly.js": "1.58.5",
    "react": "17.0.2",
    "react-bootstrap": "1.6.4",
    "react-card-flip": "1.1.5",
    "react-codemirror2": "7.2.1",
    "react-dom": "17.0.2",
    "react-phone-input-2": "2.15.0",
    "react-plotly.js": "2.5.1",
    "rfc6902": "4.0.2",
    "uuid": "8.3.2"
  },
  "devDependencies": {
    "@jupyterlab/builder": "3.4.3",
    "@types/codemirror": "0.0.109",
    "@types/google-libphonenumber": "7.4.23",
    "@types/lodash.escape": "4.0.6",
    "@types/marked": "3.0.3",
    "@types/node": "17.0.31",
    "@types/react-plotly.js": "2.5.0",
    "@types/react-router-dom": "5.3.3",
    "@types/styled-components": "5.1.25",
    "@types/uuid": "8.3.4",
    "@typescript-eslint/eslint-plugin": "4.33.0",
    "@typescript-eslint/parser": "4.33.0",
    "eslint": "7.32.0",
    "eslint-config-prettier": "6.15.0",
    "eslint-plugin-jsdoc": "38.1.6",
    "eslint-plugin-prettier": "3.4.1",
    "eslint-plugin-react": "7.29.4",
    "npm-run-all": "4.1.5",
    "prettier": "2.6.2",
    "rimraf": "3.0.2",
    "typanion": "3.9.0",
    "typescript": "4.3.5"
  },
  "resolutions": {
    "@types/react": "17.0.44"
  },
  "sideEffects": [
    "style/*.css",
    "style/index.js"
  ],
  "jupyterlab": {
    "extension": true,
    "outputDir": "jupyterlab_optumi/labextension"
  },
  "styleModule": "style/index.js"
}
