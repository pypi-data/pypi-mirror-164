import unittest
import yamal.test.sys as sys
import yamal.test.ytp as ytp

if __name__ == '__main__':
    unittest.TextTestRunner(verbosity=2).run(unittest.TestLoader().loadTestsFromModule(ytp))
    unittest.TextTestRunner(verbosity=2).run(unittest.TestLoader().loadTestsFromModule(sys))
